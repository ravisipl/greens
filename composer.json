{
    "name": "anandigreen/app",
    "type": "project",
    "description": "AnandiGreen Application",
    "require": {
        "php": "^8.1",
        "codeigniter4/framework": "^4.0",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-update-cmd": [
            "@composer dump-autoload"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}